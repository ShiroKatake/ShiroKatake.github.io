{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "noImplicitAny": false,
    "baseUrl": ".",
    "paths": {
      "/*": ["./*"],
      "assets/*": ["assets/*"],
      "components/*": ["global-components/*"],
      "context/*": ["context/*"],
      "hooks/*": ["hooks/*"],
      "models/*": ["models/*"],
      "pages/*": ["pages/*"],
      "public/*": ["public/*"],
      "styles/*": ["styles/*"],
      "theme/*": ["theme/*"],
      "utils/*": ["utils/*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx"],
  "exclude": ["node_modules"]
}
